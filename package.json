{
  "name": "ng-fake-coin",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:shell",
    "start:shell": "ng serve shell  -o --port 5000",
    "start:mfe1": "ng serve mfe1 -o --port 3000",
    "start:mfe2": "ng serve mfe2 -o --port 3001",
    "start:all": "concurrently \"npm run start:shell\" \"npm run start:mfe1\"  \"npm run start:mfe2\"",
    "build:shell": "ng build shell --configuration production",
    "build:mfe1": "ng build mfe1 --configuration production",
    "build:mfe2": "ng build mfe2 --configuration production",
    "serve:dist": "concurrently \"serve dist/shell -l 5000 -s\" \"serve dist/mfe1 -l 3000 -s\" \"serve dist/mfe2 -l 3001 -s\"",
    "build": "npm run build:shell && npm run build:mfe1 && npm run build:mfe2",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^14.0.2",
    "@angular/animations": "13.2.1",
    "@angular/cdk": "^13.2.1",
    "@angular/common": "13.2.1",
    "@angular/compiler": "13.2.1",
    "@angular/core": "13.2.1",
    "@angular/forms": "13.2.1",
    "@angular/material": "^13.2.1",
    "@angular/platform-browser": "13.2.1",
    "@angular/platform-browser-dynamic": "13.2.1",
    "@angular/router": "13.2.1",
    "@nrwl/workspace": "13.1.2",
    "concurrently": "^7.0.0",
    "prettier": "^2.3.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.2.2",
    "@angular/cli": "13.2.2",
    "@angular/compiler-cli": "13.2.1",
    "@nrwl/cli": "13.7.3",
    "@nrwl/tao": "13.7.3",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "14.14.33",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~4.0.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^13.0.0",
    "ngx-build-plus": "^13.0.1",
    "protractor": "~7.0.0",
    "serve": "^13.0.2",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "4.4.3"
  }
